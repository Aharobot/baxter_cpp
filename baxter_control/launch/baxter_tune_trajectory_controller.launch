<launch>

    alias btc="rosrun baxter_interface trajectory_controller.py"
    
  <!-- load Baxter's trajectory controller -->
  <node name="baxter_trajectory_controller" pkg="baxter_interface" type="trajectory_controller.py"
      respawn="false" output="screen" args="-l both">
  </node>

  <!-- playback file -->
  <node name="play_trajectory" pkg="joint_trajectory" type="file_playback.py" 
	args="-f $(find baxter_control)/trajectory_recordings/hold_position"/>

</launch>